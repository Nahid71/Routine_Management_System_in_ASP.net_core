@model IEnumerable<Routine_Manage.Models.Student>

@{
    ViewData["Title"] = "One";
    Layout = "~/Views/Shared/_Registry_layout.cshtml";
}

<h2>All Student of Class Four</h2>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Password)
                </td>

                <td>
                    <a asp-controller="Students" a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-controller="Students" a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-controller="Students" a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
